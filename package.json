{
  "name": "webpack-learn",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "prebuild": "npm-run-all --sequential validate-webpack:development clean:dist",
    "build": "npm-run-all --sequential build:development",
    "build:development": "webpack webpackfile.js --env.development",
    "build:production": "webpack -p webpackfile.js --env.production",
    "build:testing": "webpack webpackfile.js --env.testing",
    "prestart": "",
    "start": "npm run start:development",
    "start:development": "webpack-dev-server --env.development --content-base dist",
    "start:production": "",
    "start:testing": "",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:acceptance": "echo \"Error: no test specified\" && exit 1",
    "test:e2e": "echo \"Error: no test specified\" && exit 1",
    "test:integration": "echo \"Error: no test specified\" && exit 1",
    "test:load": "echo \"Error: no test specified\" && exit 1",
    "test:regression": "echo \"Error: no test specified\" && exit 1",
    "test:sanity": "echo \"Error: no test specified\" && exit 1",
    "test:unit": "echo \"Error: no test specified\" && exit 1",
    "clean": "npm-run-all --parallel --race clean:*",
    "clean:dist": "rimraf dist",
    "validate": "npm-run-all --parallel --race validate-webpack",
    "validate-webpack": "npm-run-all --parallel --print-name validate-webpack:*",
    "validate-webpack:development": "webpack-validator webpackfile.js --env.development",
    "validate-webpack:production": "webpack-validator webpackfile.js --env.production",
    "validate-webpack:testing": "webpack-validator webpackfile.js --env.testing"
  },
  "keywords": [],
  "author": "Evander Otieno",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "ghooks": "^1.3.2",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^2.3.0",
    "opt-cli": "^1.5.1",
    "pug": "^2.0.0-beta3",
    "pug-html-loader": "^1.0.8",
    "pug-lint": "^2.2.2",
    "pug-loader": "^1.0.2",
    "rimraf": "^2.5.3",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.0-dev.20160707",
    "webpack": "^2.1.0-beta.15",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.2"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
