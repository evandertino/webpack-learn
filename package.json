{
  "name": "webpack-learn",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.ts",
  "scripts": {
    "start": "npm-run-all --sequential validate-webpack:development start:development",
    "start:production": "webpack -p webpackfile.js --env.production",
    "start:testing": "webpack webpackfile.js --env.testing",
    "start:development": "webpack webpackfile.js --env.development",
    "test": "echo \"Error: no test specified\" && exit 1",
    "validate": "npm-run-all --parallel --race validate-webpack",
    "validate-webpack": "npm-run-all --parallel --print-name validate-webpack:*",
    "validate-webpack:production": "webpack-validator webpackfile.js --env.production",
    "validate-webpack:testing": "webpack-validator webpackfile.js --env.testing",
    "validate-webpack:development": "webpack-validator webpackfile.js --env.development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-webpack": "^6.4.1",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "npm-run-all": "^2.3.0",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-loader": "^2.1.5",
    "typescript": "^2.0.0-dev.20160707",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-validator": "^2.2.2"
  },
  "dependencies": {
    "webpack": "^2.1.0-beta.15"
  }
}
